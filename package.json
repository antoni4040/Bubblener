{
  "name": "Bubblener",
  "description": "A simple browser extension that uses LLMs to create Named-Entity Recognition (NER) bubbles.",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@mantine/core": "^8.2.1",
    "@mantine/hooks": "^8.2.1",
    "@mozilla/readability": "^0.6.0",
    "@onebeyond/license-checker": "^2.1.0",
    "@tabler/icons-react": "^3.34.1",
    "@types/chrome": "^0.1.1",
    "openai": "^5.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "typescript": "5.8.3",
    "wxt": "^0.20.6"
  }
}
